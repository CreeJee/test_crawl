               \u003C/div>\n                    \u003C/div>\n                \u003C/div>\n                \u003C!-- //inner -->\n            \u003C/div>\n            \u003Cbutton type=\"button\" class=\"poply_greyclose\" onclick=\"bigdccon_expire_pop(false)\">\u003Cem class=\"sp_img icon_grey_x\">닫기\u003C/em>\u003C/button>\n        \u003C/div>\n        \u003C!--//231121 대왕디시콘 사용기간 레이어-->\n        \u003C!--231121 nft 추천 레이어-->\n        \u003Cdiv id=\"nft_recommend_lyr\" class=\"pop_wrap type2\" style=\"display: none;left: -1px;top: 130px;z-index: 99999;\">\u003C/div>\n        \u003C!--//231121 nft 추천 레이어-->\n        \n\t \u003C/div>\n\u003C/div>\n \n\u003Cscript type=\"text/javascript\">\nvar use_bigdccon = 0;\nvar wallet_conn = 0;\n\nfunction dccon_wallet_auth() {\n    $.ajax({\n      url: '/ajax/nft_ajax/auth?type=dccon',\n      type: 'GET',\n      cache: false,\n      async: false,\n      dataType: 'json',\n      success: function(obj) {\n          if(obj.result == 'success') {\n            let qr_box = '\u003Cdiv class=\"bbox\">\u003Cb class=\"middlet\">지갑 연결을 위해 휴대전화로\u003Cbr>QR코드를 스캔해 주세요.\u003C/b>\u003C/div>'+obj.html;\n            $('#dccon_wallet_auth_layer .klip_qrbox .inr').prepend(qr_box);\n            $('.dccon_wallet_auth').show();\n          }\n          else {\n            alert('다시 시도해 주세요');\n          }\n      }\n    });\n}\n\nfunction dccon_wallet_done(obj) {\n\t\n\tconsole.log('dccon_wallet_done');\n\tconsole.log(obj);\n    wallet_conn = 1;\n    \n    $('.dccon_wallet_auth').hide();\n    $('#dccon_wallet_auth_layer #qrbox').remove();\n    \n    $('.wallet_box .btn_wallet.on').hide();\n    $('.wallet_box .btn_wallet.off').show();\n    \n    $('.wallet_box .tip_box_bd').hide();\n    \n    if(typeof(obj.bigdccon) != 'undefined' && obj.bigdccon && obj.bigdccon > Math.floor(Date.now() / 1000)) {\n      use_bigdccon = 1;\n      \n      $('.dccon_list_btm .wallet_box .btn_wallet.txton').show();\n      $('.dccon_list_btm .wallet_box .btn_wallet.time').sh